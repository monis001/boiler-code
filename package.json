{
  "name": "typescript-boilerplate",
  "version": "1.0.0",
  "description": "boilerplate for typescript",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "typescript": "^3.1.3",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "tsc index.ts",
    "clean": "npm run clean-build-steps && rm -rf dist",
    "tsc": "./node_modules/.bin/tsc",
    "babel": "./node_modules/.bin/babel build-tsc --out-dir build-babel --source-maps",
    "webpack": "webpack && cp public/* dist",
    "clean-build-steps": "rm -rf build-tsc build-babel",
    "build": "npm run clean && npm run tsc && npm run babel && npm run webpack && npm run clean-build-steps"
  },
  "author": "monis01",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/monis01/boiler-code/issues"
  },
  "homepage": "https://github.com/monis01/boiler-code#readme"
}
